n1 = float(input('forneça a primeira nota:'))
n2 = float(input('forneça a segunda nota:'))
n3 = float(input('forneça a terceira nota:'))

med = (n1 + n2 + n3) / 3

if med == 10:
  print('APROVADO COM DISTINÇÃO.')

if 7 <= med < 10:
  print('a média das notas é:',med,'APROVADO.')

if 0 <= med < 7:
  print('a média das notas é:',med,'REPROVADO.')
