n = int(input('forneça um numero correspondente a ordem dos dias da semana:'))

if n == 1:
  print('Domingo')

if n == 2:
  print('Segunda')

if n == 3:
  print('Terça')

if n == 4:
  print('Quarta')

if n == 5:
  print('Quinta')

if n == 6:
  print('Sexta')

if n == 7:
  print('Sábado')

if n > 7:
  print('erro.',n ,'não corresponde a um dia da semana.')

